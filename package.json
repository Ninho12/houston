{
  "private": true,
  "name": "@eduzz/houston-workspaces",
  "version": "0.11.8",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn ultra -r -b build",
    "prestart": "yarn ultra -r -b --filter 'packages/*' build",
    "start": "yarn ultra -r start",
    "prepare": "yarn husky install",
    "publish": "NPM_AUTH_TOKEN=$NPM_AUTH_TOKEN sh ./scripts/publish-all.sh",
    "change-version": "sh ./scripts/change-version.sh",
    "clean": "git add . && git clean -f -x",
    "lint": "yarn ultra -r lint"
  },
  "devDependencies": {
    "husky": "5.1.1",
    "inquirer": "7.3.3",
    "ora": "5.3.0",
    "semver": "7.3.4",
    "typescript": "4.2",
    "ultra-runner": "3.9.0"
  },
  "resolutions": {
    "@babel/core": "7.13"
  },
  "ultra": {
    "concurrent": [
      "lint"
    ]
  }
}
