{
  "plugins": [
    "eslint-plugin"
  ],
  "ignorePatterns": "",
  "extends": [
    "plugin:eslint-plugin/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    }
  },
  "rules": {
    "quotes": [
      "error",
      "single",
      {
        "avoidEscape": true
      }
    ]
  }
}