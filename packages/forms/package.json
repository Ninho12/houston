{
  "name": "@eduzz/houston-forms",
  "version": "0.0.1",
  "description": "Houston Forms",
  "main": "index.js",
  "types": "./index.d.ts",
  "author": "Eduzz Team",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/eduzz/houston",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduzz/houston"
  },
  "bugs": {
    "url": "https://github.com/eduzz/houston/issues"
  },
  "scripts": {
    "start": "yarn tsc -w --preserveWatchOutput",
    "clean": "git add . && git clean -f -x",
    "build": "yarn clean && tsc -p .",
    "prepare": "yarn build",
    "lint": "yarn eslint \"./**/*.ts\" \"./**/*.tsx\"",
    "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
    "post-merge": "yarn"
  },
  "dependencies": {
    "@eduzz/houston-core": "*",
    "formik": "^2.2.5",
    "react": "^16.13.1",
    "react-use-observable": "^1.0.2",
    "rxjs": "^6.6.3",
    "yup": "^0.31.0",
    "@types/yup": "^0.29.9"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "devDependencies": {
    "@eduzz/eslint-config-houston": "*",
    "@types/react": "16.9.55",
    "concurrently": "5.3.0"
  }
}
