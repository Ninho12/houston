{
  "name": "@eduzz/houston-hooks",
  "description": "Eduzz Houston Hooks",
  "version": "0.10.2",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "Eduzz Team",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/eduzz/houston",
  "repository": {
    "type": "git",
