{
  "name": "@eduzz/houston-ui",
  "description": "Eduzz Houston Components UI",
  "version": "0.0.48",
  "main": "./index.js",
  "types": "./index.d.ts",
  "author": "Eduzz Team",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/eduzz/houston",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduzz/houston"
  },
  "bugs": {
    "url": "https://github.com/eduzz/houston/issues"
  },
  "scripts": {
    "start": "yarn tsc -w --preserveWatchOutput",
    "clean": "git add . && git clean -f -x",
    "build": "yarn clean && tsc -p .",
    "prepare": "yarn build",
    "lint": "yarn eslint \"./**/*.ts\" \"./**/*.tsx\"",
    "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
    "post-merge": "yarn"
  },
  "dependencies": {
    "@material-ui/core": "4",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "4",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "@material-ui/core": "4",
    "@material-ui/lab": "4",
    "@material-ui/styles": "4",
    "react": ">=16.8"
  },
  "devDependencies": {
    "@types/react": "16.9.55",
    "concurrently": "5.3.0",
    "@eduzz/eslint-config-houston": "*",
    "typescript": "4.0.5"
  }
}